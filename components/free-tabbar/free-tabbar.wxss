.free-tabbar{
  display: flex;
  position: relative;
  align-items: center;
  min-height: 120rpx;
  justify-content: space-between;
}
.free-tabbar.tabbar{
  padding: 0;
  height: calc(120rpx + env(safe-area-inset-bottom) / 2);
  padding-bottom: calc(env(safe-area-inset-bottom) / 2);
  background: linear-gradient(0deg, rgba(255, 255, 255, 0.79), rgba(255, 255, 255, 0.79)), rgba(237, 237, 237, 0.7);
backdrop-filter: blur(10px);
}
.free-tabbar .free-tabbar-item{
  display: flex;
  align-items: center;
  height: 100%;
  justify-content: center;
  max-width: 100%;
}
.free-tabbar .free-tabbar-item:first-child {
  margin-left: 30rpx;
  font-size: 30rpx;
}
.free-tabbar.tabbar .free-tabbar-item{
  font-size: 22rpx;
  position: relative;
  flex: 1;
  /* text-align: center; */
  padding: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: auto;
  line-height: 1;
  margin: 0;
  overflow: initial;
}

.free-tabbar.tabbar .free-tabbar-item.free-action .free-icon{
  position: absolute;
  width: 80rpx;
  z-index: 2;
  height: 80rpx;
  border-radius: 50%;
  line-height: 70rpx;
  font-size: 50rpx;
  top: -22rpx;
  left: 0;
  right: 0;
  margin: auto;
  padding: 0;
  /* padding-bottom: 7rpx; */
}
.free_tabbar_text{
  font-size:20rpx;
  line-height: 22rpx;
  font-weight: 400;
}
.free-tabbar.tabbar .free-tabbar-item .free-icon{
  width: 100rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: auto;
  /* margin: 0 auto; */
  /* text-align: center;   */
}
