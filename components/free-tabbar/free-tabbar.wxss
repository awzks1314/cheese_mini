.free-tabbar-tag{
  position: absolute;
  top: 0rpx;
  right: 10rpx;
  background: #dd514c;
  border-radius: 200rpx;
  z-index: 9;
  color: #fff;
  font-size: 20rpx;
  padding: 0 10rpx;
  height: 28rpx;
  display: inline-flex;
  align-items: center;
}
