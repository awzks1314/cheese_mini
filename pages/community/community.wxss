.commit_bottom_inputBox{
  position: fixed;
  width: 100%;
  height: 100%;
  top:0;
  left: 0;
  z-index: 99;
}
.commit_bottom_input{
  position: fixed;
  z-index: 101;
  bottom: -120rpx;
  width: 100%;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  background-color: #fff;
  transition: all .3s ease-in-out;
}
.inputShow{
  bottom: 0;
}
.inputClose{
  bottom: -120rpx;
}
.commit_bottom_inputStyle{
  display: flex;
    align-items: center;
    justify-items: center;
    background: #f5f5f5;
    line-height: 32px;
    height: 64rpx;
    font-size: 24rpx;
    color: #333333;
    flex: 1;
    overflow: hidden;
    margin-right: 30rpx;
    padding: 0 30rpx;
    border-radius: 50rpx;
}

